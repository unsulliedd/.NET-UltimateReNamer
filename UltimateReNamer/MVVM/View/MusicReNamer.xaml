<UserControl x:Class="UltimateReNamer.MVVM.View.MusicReNamer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UltimateReNamer.MVVM.View"
             xmlns:vm="clr-namespace:UltimateReNamer.MVVM.ViewModel"
             d:DataContext="{d:DesignInstance vm:MusicViewModel, IsDesignTimeCreatable=True}"
             mc:Ignorable="d" 
             d:DesignHeight="650" d:DesignWidth="420">
    <Grid>
        <TextBlock Text="{Binding Title}" Margin="10,10,0,0" FontSize="20" 
                   HorizontalAlignment="Center" VerticalAlignment="Center"  
                   Foreground="AliceBlue"/>
    </Grid>
</UserControl>

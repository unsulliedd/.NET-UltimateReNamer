<Application x:Class="UltimateReNamer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UltimateReNamer"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Style/MaterialColorPalette.xaml"/>
                <ResourceDictionary Source="./Style/Menu/MenuButtonText.xaml"/>
                <ResourceDictionary Source="./Style/Menu/MenuButtonIcon.xaml"/>
                <ResourceDictionary Source="./Style/Menu/MenuButton.xaml"/>
                <ResourceDictionary Source="./Style/ScrollBar.xaml"/>
                <ResourceDictionary Source="./Core/DataTemplate.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

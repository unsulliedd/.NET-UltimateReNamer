<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:UltimateReNamer.MVVM.ViewModel"
                    xmlns:view="clr-namespace:UltimateReNamer.MVVM.View">

    <DataTemplate DataType="{x:Type vm:MainWindowViewModel}">
        <view:Home/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DocumentViewModel}">
        <view:DocumentReNamer/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PhotoViewModel}">
        <view:PhotoReNamer/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MusicViewModel}">
        <view:MusicReNamer/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:VideoViewModel}">
        <view:VideoReNamer/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OtherViewModel}">
        <view:OtherReNamer/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <view:Settings/>
    </DataTemplate>


    
</ResourceDictionary>